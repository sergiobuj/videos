<h1>New cast</h1>

<%= error_messages_for :cast %>
<% form_for(@cast) do |f| %>

  <p>
    <b>Movie code</b>
    <%= f.text_field :movie_code %>

    <b>Actor actress name</b>
    <%= f.text_field :actor_actress_name %>
  </p>

  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', casts_path %>

